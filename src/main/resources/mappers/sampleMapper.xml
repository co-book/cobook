<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="org.ebook.cobook.mapper.SampleMapper">

	<select id="findNickName" resultType="map">
	SELECT member_no, nickname FROM member
	WHERE member_no = #{member_no}
	</select>
	

</mapper>