													</c:when>
													<c:otherwise>
														<li><a href="#"><i class="fa fa-star-half-o" aria-hidden="true"></i></a></li>
													</c:otherwise>
												</c:choose>
											</c:forEach>
											</ul>
										</div>
										<div class="clearfix"></div>
									</div>
								</div>
								
							</div>
